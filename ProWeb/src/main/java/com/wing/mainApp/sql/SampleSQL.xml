<?xml version="1.0" encoding="UTF-8"?>

<!-- DTD 파일을 설정한다. -->
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<!--  루트 태그를 작성한다. -->
<mapper namespace="sample">
	<!--  이안에 필요한 질의 명령 만들기 -->
	<select id="total" resultType="int">
		Select
			Count(*) AS TOTAL
		FROM
			Member
	</select>
</mapper>