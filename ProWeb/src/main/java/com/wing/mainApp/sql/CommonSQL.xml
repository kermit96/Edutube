<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="comm">

	<!-- 과목 전체 출력 -->
	<select id="getSubList" resultType="subdata" >
		SELECT
			sub_code AS subcode,
			sub_name AS subname
		FROM
			subject	
	</select>
	
</mapper>